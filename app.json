{
  "expo": {
    "name": "PasarVisual",
    "slug": "pasar-visual",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/Assets/icon.png",
    "splash": {
      "image": "./src/Assets/images/pasarvisual-logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "backgroundColor": "#FFFFFF"
    },
    "android": {
      "package": "com.blackjack.pasarvisual",
      "versionCode": 1,
      "adaptiveIcon": {
        "foregroundImage": "./src/Assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "./src/Assets/favicon.png"
    }
  }
}
